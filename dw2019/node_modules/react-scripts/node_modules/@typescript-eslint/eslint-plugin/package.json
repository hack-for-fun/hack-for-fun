{
  "name": "@typescript-eslint/eslint-plugin",
  "version": "2.6.1",
  "description": "TypeScript plugin for ESLint",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "typescript"
  ],
  "engines": {
    "node": "^8.10.0 || ^10.13.0 || >=11.10.1"
  },
